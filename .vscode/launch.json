  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": ["--flavor", "dev", "--target", "lib/main_dev.dart"]
    },
    {
      "name": "Flutter UAT",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_aut.dart",
      "args": ["--flavor", "uat", "--target", "lib/main_aut.dart"]
    },
    {
      "name": "Flutter Prod",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--flavor", "prod", "--target", "lib/main.dart"]
    }
  ]
}
